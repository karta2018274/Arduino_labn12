#include <SoftwareSerial.h>
SoftwareSerial Serial1(3,2);
void setup()
{
  Serial1.begin(115200);
  Serial.begin(115200);
  Serial.println("start");
}
void loop()
{
  
  if(Serial1.available())
    {
      Serial.write(Serial1.read());
    }
   if(Serial.available())
   {
     char chars = Serial.read();
     Serial1.write(chars);
   }
    
  
}
